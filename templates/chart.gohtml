{{ define "chart" }}
<svg id="{{ .Name | ToLower }}-chart" xmlns="http://www.w3.org/2000/svg" width="800" height="300">
    <path d="{{.Path}}" fill="none" stroke="#ff1a1a" stroke-width="3"/>
    {{range .Points}}
        <circle cx="{{.X}}" cy="{{.Y}}" r="4" fill="#fff" stroke="#ff1a1a" stroke-width="2"/>
        <text x="{{.X}}" y="280" font-size="10" text-anchor="middle">{{.Label}}</text>
    {{end}}
</svg>
{{ end }}